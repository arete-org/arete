{
    "name": "@arete/discord-bot",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "exports": {
        ".": "./dist/index.js",
        "./src/utils/openaiService.js": "./dist/src/utils/openaiService.js",
        "./src/utils/response/metadata.js": "./dist/src/utils/response/metadata.js",
        "./src/utils/RateLimiter.js": "./dist/src/utils/RateLimiter.js"
    },
    "scripts": {
        "prestart": "pnpm build",
        "start": "node --es-module-specifier-resolution=node dist/index.js",
        "dev": "pnpm exec cross-env NODE_ENV=development pnpm exec tsx watch src/index.ts",
        "build": "pnpm --filter @arete/backend build && pnpm clean && tsc && pnpm copy-files",
        "clean": "rimraf dist",
        "copy-files": "xcopy /s /e /i /y src\\*.* dist\\src\\ 2>nul && xcopy /s /e /i /y src\\commands\\*.* dist\\commands\\ 2>nul && xcopy /s /e /i /y src\\events\\*.* dist\\events\\ 2>nul && xcopy /s /e /i /y src\\utils\\*.* dist\\utils\\ 2>nul || echo No files to copy",
        "watch": "tsc -w"
    },
    "dependencies": {
        "@arete/backend": "workspace:*",
        "@discordjs/voice": "catalog:",
        "@openai/agents": "catalog:",
        "@sapphire/shapeshift": "catalog:",
        "body-parser": "catalog:",
        "cloudinary": "catalog:",
        "date-fns": "catalog:",
        "discord.js": "catalog:",
        "dotenv": "catalog:",
        "express": "catalog:",
        "flyio": "catalog:",
        "js-yaml": "catalog:",
        "linkify-it": "^5.0.0",
        "mime-types": "catalog:",
        "node-fetch": "catalog:",
        "only": "catalog:",
        "openai": "catalog:",
        "opusscript": "catalog:",
        "prism-media": "catalog:",
        "reindex": "catalog:",
        "remark-parse": "^11.0.0",
        "unified": "^11.0.5",
        "unist-util-visit": "^5.0.0",
        "winston": "catalog:",
        "ws": "catalog:"
    },
    "devDependencies": {
        "@types/body-parser": "catalog:",
        "@types/express": "catalog:",
        "@types/linkify-it": "^5.0.0",
        "@types/mdast": "^4.0.4",
        "@types/node": "catalog:",
        "@types/unist": "^3.0.3",
        "copyfiles": "catalog:",
        "cross-env": "catalog:",
        "rimraf": "catalog:",
        "ts-node-dev": "catalog:",
        "tsx": "catalog:",
        "typescript": "catalog:"
    }
}
