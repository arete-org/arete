{
  "name": "@ai-assistant/discord-bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "dev": "npx cross-env NODE_ENV=development npx tsx watch src/index.ts",
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf dist",
    "copy-files": "xcopy /s /e /i /y src\\*.* dist\\src\\ 2>nul && xcopy /s /e /i /y src\\commands\\*.* dist\\commands\\ 2>nul && xcopy /s /e /i /y src\\events\\*.* dist\\events\\ 2>nul && xcopy /s /e /i /y src\\utils\\*.* dist\\utils\\ 2>nul || echo No files to copy",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@ai-assistant/shared": "file:../shared",
    "@openai/agents": "^0.1.8",
    "@pinecone-database/pinecone": "^6.1.2",
    "body-parser": "^2.2.0",
    "cloudinary": "^2.7.0",
    "date-fns": "^4.1.0",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "flyio": "^0.6.14",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "only": "^0.0.2",
    "openai": "^5.12.2",
    "opusscript": "^0.0.8",
    "prism-media": "^1.3.5",
    "reindex": "^0.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^10.1.0",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
